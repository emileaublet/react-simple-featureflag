{
  "name": "react-simple-featureflag",
  "description": "Simple React utility to manage feature flag.",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "react": "^17.0.2"
  },
  "author": "Emile Aublet <emileaublet@gmail.com>",
  "keywords": [
    "react",
    "featureflag",
    "feature",
    "flag",
    "toggle"
  ],
  "main": "dist/components/index.js",
  "module": "dist/components/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emileaublet/react-simple-featureflag"
  },
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5"
  }
}
